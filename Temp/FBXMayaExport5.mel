print "Starting maya loading and fbx conversion \n";
	
if (`file -q -exists "/Users/mac/Desktop/unity-whalemuseum/Assets/models/WaterFountain.mb"`)
{		
	file -o "/Users/mac/Desktop/unity-whalemuseum/Assets/models/WaterFountain.mb";
	FBXExportEmbeddedTextures -v false;
	// FBXExportHardEdges -v true;
	FBXExportApplyConstantKeyReducer -v false;
	FBXExportShowUI -v false;

	print "Before fbx export\n";
	FBXExport -f "/Users/mac/Desktop/unity-whalemuseum/Temp/ExportedFBXFile.fbx";
	print "after fbx export\n";
}
else
	print "Could not open Maya file.";

sysFile -delete "/Users/mac/Desktop/unity-whalemuseum/Temp/CommandPipe";

$donepipeKill = `fopen "/Users/mac/Desktop/unity-whalemuseum/Temp/SyncPipeKill" "w"`;
fwrite $donepipeKill "Done";
fclose $donepipeKill;
